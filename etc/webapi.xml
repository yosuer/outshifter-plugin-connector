<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
  <route method="POST" url="/V1/outshifter-connect/order">
    <service class="Outshifter\Outshifter\Api\OrderServiceInterface" method="save" />
    <resources>
      <resource ref="anonymous" />
    </resources>
  </route>
  <route method="GET" url="/V1/outshifter-connect/currency">
    <service class="Outshifter\Outshifter\Api\ConfigServiceInterface" method="getCurrency" />
    <resources>
      <resource ref="anonymous" />
    </resources>
  </route>
</routes>